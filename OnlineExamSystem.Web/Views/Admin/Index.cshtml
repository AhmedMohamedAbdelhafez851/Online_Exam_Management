<!-- OnlineExamSystem.Web/Views/Admin/Index.cshtml -->
@model IEnumerable<OnlineExamSystem.Domains.Entities.Exam>
<h2>Exams</h2>
<a asp-action="Create" class="btn btn-primary">Create New Exam</a>
<table class="table">
    <thead><tr><th>Title</th><th>Created By</th></tr></thead>
    <tbody>
        @foreach (var exam in Model)
        {
            <tr><td>@exam.Title</td><td>@exam.CreatedBy</td></tr>
        }
    </tbody>
</table>